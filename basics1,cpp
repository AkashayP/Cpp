#include<iostream>
using namespace std;
int main(){
    cout<<"hello"<<endl;  // endl used to end the line.
    cout<<endl;
    cout<<"Hello my name is Akashay Prakash Singh.";
    cout<< endl <<"\n";
    cout<<"Hello PW"<<endl<<"Hello CW";    // After using endl we can write instructions for the next line.

} 
